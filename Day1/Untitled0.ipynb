        "import torch\n",
        "\n",
        "d = [[1, 2], [3,4]]\n",
        "x = torch.tensor(d)\n",
        "print(x)\n",
        "t = x\n",
        "t[:,1] = 0\n",
        "print(t)\n",
        "y1 = t @ t.T\n",
        "y2 = t.matmul(t.T)\n",
        "\n",
        "print(y1)\n",
        "print(y2)"
      ]
